<application>
  <component name="LanguageInjectionConfiguration">
    <option name="INSTRUMENTATION" value="ASSERT" />
    <option name="LANGUAGE_ANNOTATION" value="org.intellij.lang.annotations.Language" />
    <option name="PATTERN_ANNOTATION" value="org.intellij.lang.annotations.Pattern" />
    <option name="SUBST_ANNOTATION" value="org.intellij.lang.annotations.Subst" />
    <option name="RESOLVE_REFERENCES" value="true" />
    <injection language="JavaScript 1.8" injector-id="xml">
      <display-name>Hyperlink/@onMouseClicked</display-name>
      <place><![CDATA[xmlAttribute().withLocalName(string().equalTo("onMouseClicked")).withParent(xmlTag().withLocalName(string().equalTo("Hyperlink")).withNamespace(string().equalTo("http://javafx.com/fxml")))]]></place>
    </injection>
    <injection language="JavaScript 1.8" injector-id="xml">
      <display-name>define</display-name>
      <place><![CDATA[xmlTag().withLocalName(string().equalTo("define")).withNamespace(string().equalTo("http://javafx.com/fxml/1"))]]></place>
    </injection>
    <injection language="JAVA" injector-id="xml">
      <display-name>image/@url</display-name>
      <place><![CDATA[xmlAttribute().withLocalName(string().equalTo("url")).withParent(xmlTag().withLocalName(string().equalTo("image")).withNamespace(string().equalTo("http://javafx.com/fxml")))]]></place>
    </injection>
  </component>
</application>